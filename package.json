{
  "name": "compat-check",
  "version": "1.0.0",
  "description": "Check compatibility of npm packages",
  "license": "ISC",
  "author": "Estev√£o",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "keywords": [
    "cli",
    "npm"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint:fix": "biome check --write --error-on-warnings"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "compat-check": "link:",
    "kleur": "^4.1.5",
    "npm-package-arg": "^12.0.2",
    "npm-registry-fetch": "^18.0.2",
    "ora": "^8.2.0",
    "semver": "^7.7.2",
    "zod": "^3.25.17",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.15.19",
    "@types/npm-package-arg": "^6.1.4",
    "@types/npm-registry-fetch": "^8.0.8",
    "@types/semver": "^7.7.0",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "compat-check": "link:"
    }
  }
}